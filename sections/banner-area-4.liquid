<!-- Banner Area Start -->
<div class="banner-area" id="section-{{ section.id }}">
  <div class="{% if section.settings.container_style == 'style_1' %}container {% else %} container-fluid p-0{% endif %}">
    <div class="row no-gutters">
      {% for block in section.blocks %}
      {% case block.type %}
      {% when 'double_banner' %}
      <div class="col-lg-{{ block.settings.double_banner_coloum }} d-md-flex d-lg-block" id="block-{{ block.id }}">
        <div class="banner-box banner-type-7 {% if block.settings.text_style == 'text_style_1' %} banner-1 {% else %} banner-4 {% endif %} banner-hover-5">
          <div class="banner-inner">
            <div class="banner-image">
              <img src="{% if block.settings.banner_img_1 %}{{ block.settings.banner_img_1 | img_url: 'master' }}{% else %}http:\/\/via.placeholder.com\/635x395{% endif %}" alt="{{ block.settings.banner_img_1.alt }}">
            </div>
            {% if block.settings.banner_button_1 != '' or block.settings.banner_title_2 != '' or block.settings.banner_sub_title_2 != '' or block.settings.banner_description_2 != '' %}
            <div class="banner-info single-banner-2">
              <div class="banner-info--inner">
                <p class="banner-title-3 color--white">{{ block.settings.banner_sub_title_2 }}</p>
                <p class="banner-title-7 font-bold">{{ block.settings.banner_title_2 }}</p>
                <p class="banner-title-8 color--white">{{ block.settings.banner_description_2 }}</p>
              </div>
              <a href="{{ block.settings.banner_button_1_url }}" class="btn btn-style-1 btn-medium btn-2">{{ block.settings.banner_button_1 }}</a>
            </div>
            {% endif %}
          </div>
        </div>
        <div class="banner-box banner-type-7 {% if block.settings.text_2_style == 'text_style_3' %} banner-2 {% else %} banner-5 {% endif %} banner-hover-5">
          <div class="banner-inner">
            <div class="banner-image">
              <img src="{% if block.settings.banner_img_2 %}{{ block.settings.banner_img_2 | img_url: 'master' }}{% else %}http:\/\/via.placeholder.com\/635x395{% endif %}" alt="{{ block.settings.banner_img_2.alt }}">
            </div>
            {% if block.settings.banner_button_2 != '' or block.settings.banner_title_3 != '' or block.settings.banner_sub_title_3 != '' or block.settings.banner_description_3 != '' %}
            <div class="banner-info single-banner-3">
              <div class="banner-info--inner">
                <p class="banner-title-3 color--white">{{ block.settings.banner_description_3 }}</p>
                <p class="banner-title-9 color--white">{{ block.settings.banner_sub_title_3 }}</p>
                <p class="banner-title-7">{{ block.settings.banner_title_3 }}</p>
              </div>
              <a href="{{ block.settings.banner_button_2_url }}" class="btn btn-style-1 btn-medium btn-2">{{ block.settings.banner_button_2 }}</a>
            </div>
            {% endif %}
          </div>
        </div>
      </div>
      {% when 'banner' %}
      <div class="col-lg-{{ block.settings.banner_coloum }} col-md-12" id="block-{{ block.id }}">
        <div class="banner-box banner-type-7 banner-3 banner-hover-5">
          <div class="banner-inner">
            <div class="banner-image">
              <img src="{% if block.settings.banner_img %}{{ block.settings.banner_img | img_url: 'master' }}{% else %}http:\/\/via.placeholder.com\/635x785{% endif %}" alt="{{ block.settings.banner_img.alt }}">
            </div>
            {% if block.settings.banner_button != '' or block.settings.banner_title != '' or block.settings.banner_sub_title != '' or block.settings.banner_description != '' %}
            <div class="banner-info single-banner">
              <div class="banner-info--inner">
                <p class="banner-title-8 color--white">{{ block.settings.banner_sub_title }}</p>
                <p class="banner-title-7 font-bold">{{ block.settings.banner_title }}</p>
                <p class="banner-title-3 color--white">{{ block.settings.banner_description }}</p>
              </div>
              <a href="{{ block.settings.banner_button_url }}" class="btn btn-style-1 btn-medium btn-2">{{ block.settings.banner_button }}</a>
            </div>
            {% endif %}
          </div>
        </div>
      </div>
       {% endcase %}
      {% endfor %}
    </div>
  </div>
</div>

<!-- Banner Area End -->



