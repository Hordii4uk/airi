<style>
  
    #section-{{ section.id }} .header-inner,#section-{{ section.id }} .header-inner-2{
    padding-top:{{ section.settings.section_padding_top_1 }}rem;
  }

  #section-{{ section.id }} .header-inner,#section-{{ section.id }} .header-inner-2{
    padding-bottom:{{ section.settings.section_padding_bottom_1 }}rem;
  }


  @media (min-width: 768px) and (max-width: 991px){

   #section-{{ section.id }} .header-inner,#section-{{ section.id }} .header-inner-2{
      padding-top:{{ section.settings.section_padding_top_md_1 }}rem;
    }

    #section-{{ section.id }} .header-inner,#section-{{ section.id }} .header-inner-2{
      padding-bottom:{{ section.settings.section_padding_bottom_md_1 }}rem;
    }

  }
  @media (max-width: 767px) {

    #section-{{ section.id }} .header-inner,#section-{{ section.id }} .header-inner-2{
      padding-top:{{ section.settings.section_padding_top_xs_1 }}rem;
    }

    #section-{{ section.id }} .header-inner,#section-{{ section.id }} .header-inner-2{
      padding-bottom:{{ section.settings.section_padding_bottom_xs_1 }}rem;
    }
  }

  
     #section-{{ section.id }} .sticky-header{
    padding-top:{{ section.settings.section_padding_top_4 }}rem;
  }

  #section-{{ section.id }} .sticky-header{
    padding-bottom:{{ section.settings.section_padding_bottom_4 }}rem;
  }


  @media (min-width: 768px) and (max-width: 991px){

    #section-{{ section.id }} .sticky-header{
      padding-top:{{ section.settings.section_padding_top_md_4 }}rem;
    }

    #section-{{ section.id }} .sticky-header{
      padding-bottom:{{ section.settings.section_padding_bottom_md_4 }}rem;
    }

  }
  @media (max-width: 767px) {

    #section-{{ section.id }} .sticky-header{
      padding-top:{{ section.settings.section_padding_top_xs_4 }}rem;
    }

    #section-{{ section.id }} .sticky-header{
      padding-bottom:{{ section.settings.section_padding_bottom_xs_4 }}rem;
    }
  }
  
  
  
  .header.header-transparent,.header {
    background: {{ section.settings.header_bg_color }};
  }
  
   .header .sticky-header {
    background: {{ section.settings.header_bg_color_2 }};
  }
  
  .theme-logo img{
    max-width: {{ section.settings.logo_width }}px;
  }
</style>




